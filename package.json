{
  "name": "transaction-describer",
  "version": "0.1.5",
  "description": "Transaction Describer",
  "main": "src/express-index.ts",
  "scripts": {
    "start": "pnpm pm2",
    "build": "pnpm crfa && tsc -b tsconfig.json",
    "dev": "pnpm crfa && tsx --env-file=.env --watch src/express-index.ts",
    "dev-express": "pnpm crfa && nodemon src/express-index.ts -w . -w .env -e ts,js,mjs,json,env",
    "dev-effect": "pnpm crfa && tsx --env-file=.env --watch src/effect-main.ts",
    "lint": "pnpm crfa && eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\" --fix",
    "test": "pnpm crfa && vitest run --coverage",
    "pm2": "pnpm build && pm2-runtime start out/express-index.js --env production 2>/dev/null",
    "node": "ts-node src/express-index.ts",
    "crfa": "test -d ./crfa-offchain-data-registry/.git || git clone https://github.com/Cardano-Fans/crfa-offchain-data-registry.git && git -C ./crfa-offchain-data-registry/ pull",
    "lint-docs": "redocly lint ./openapi.yaml --format stylish",
    "build-docs": "pnpm lint-docs && redocly build-docs ./openapi.yaml -o ./docs/index.html",
    "postinstall": "pnpm crfa"
  },
  "author": "Ariady Putra",
  "license": "MIT",
  "dependencies": {
    "@blockfrost/blockfrost-js": "6.0.0",
    "@blockfrost/openapi": "0.1.78",
    "@effect/eslint-plugin": "0.3.2",
    "@effect/experimental": "0.36.2",
    "@effect/platform": "0.72.2",
    "@effect/platform-node": "0.68.2",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@lucid-evolution/lucid": "0.4.29",
    "@typescript-eslint/eslint-plugin": "8.26.0",
    "@typescript-eslint/parser": "8.26.0",
    "dotenv": "16.5.0",
    "effect": "3.12.2",
    "eslint": "9.10.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-codegen": "0.28.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "express": "4.21.2",
    "express-openapi": "12.1.3",
    "express-rate-limit": "7.5.0",
    "js-yaml": "4.1.0",
    "node-cache": "5.1.2",
    "pm2": "6.0.5",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@effect/language-service": "0.3.0",
    "@effect/vitest": "0.16.2",
    "@redocly/cli": "1.34.3",
    "@types/express": "4.17.22",
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.14.1",
    "@vitest/coverage-v8": "3.1.4",
    "nodemon": "3.1.10",
    "prettier": "3.3.3",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vitest": "3.1.4"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "10.x"
  }
}